#main-container{
margin-left: 30px;

}
#profile-photo{
  border-radius: 45px;
  padding: 10px;
}

#user-name{
  margin-top: 20px;
}

#results {
  font-size: 14px;
  font-weight: bold;
  border-bottom:1px solid #ddd;
  margin-bottom: 10px;
  text-align: left;
  vertical-align: text-top;
  min-height: 50px;
  margin-top: 30px;
  margin-left: -10px;
  width: 50%;
  display: inline-block;
}

#interim_span{
  color: grey;
}

#final_span{
  color: black;
}

#logo{
  width: 60px;
  margin: 0px;
  margin-top: -10px;
  z-index: 99;
}

li{
  text-decoration: none
}

#navbar{
  border: 1px solid grey;
  position: fixed;
  width: 100%;
  /*margin-top: -20px;*/
  height: 80px;
  border-top: none;

}
#treemap{
  margin-left: 40px
}
.treemap-node {
  box-sizing: border-box;
  border: 1px solid #fff;
  padding: 0.5em;
  overflow: hidden;
  color: #fff;
  text-shadow: 1px 1px #333;
}
.treemap-legend {
  width: 500px;
  padding: 0 1em;
  box-sizing: border-box;
  border: 1px solid #fff;
  overflow: hidden;
  color: #fff;
  text-shadow: 1px 1px #333;
  background-image: -webkit-linear-gradient(left, #93C 0%, cyan 20%, lime 40%, yellow 60%, orange 80%, red 100%);
}
.treemap-legend-end {
  float: right;
}
.loader {
  position: fixed;
  left: 0px;
  top: 0px;
  width: 100%;
  margin-top: 50px;
  height: 100%;
  z-index: 999;
  background: url('http://www.hannahreed.co.uk/images/loading.gif') 50% 50% no-repeat rgb(249,249,249);
}
#range-slider{
  background-color: black;
}
#welcome{
  margin-top: 130px;
  margin-left: 20px;
  color: rgba(0,0,0,.4);
}

#calendar{
  margin-left: 30px;
}
#welcome-background{
  padding: 100px;
  position:fixed;
  width:100%;
  height:100%;
  background: url('http://mediad.publicbroadcasting.net/p/kuow/files/201311/elephant-herd.jpg') no-repeat;
  background-size: 100%;
}
.devise-alerts {
  position:  fixed;
  margin-left: 30px;
}
#scroller-icon{
  font-size: 35px;
  color: rgba(0,0,0,.3);
}
#scroller{
  position: fixed;
  right: 0px;
  bottom: 0px;
  margin: 50px;
  z-index: 10000;
}
.red{
}
#google-results{
  margin-left: 30px;
}
#google-result{
  border-bottom: 1px solid #ddd;
  width: 1100px;
  margin: 10px;
}
#alchemy{
  margin: 10px;
  border-radius: 5px;
  background: white;
   font-family: "Lucida Grande";
}
#datetimebutton{
  margin: 10px;
  margin-left: 10px;
  border-radius: 5px;
  background: white;
   font-family: "Lucida Grande";
}
#tooltip {
  border-radius: 5px;
  background: white;
  float: right;
  font-family: "Lucida Grande";
  margin: 5px 20px 0 0;
}
#transcript{
  margin: 10px;
  margin-left: 10px;
  border-radius: 5px;
  background: white;
   font-family: "Lucida Grande";
}
#live{
  background: "red";
  margin: 10px;
  margin-left: 10px;
  border-radius: 5px;
  background: white;
  font-family: "Lucida Grande";
}
#transcript-box{
  margin: 20px;
  border: 1px solid #ddd;
  padding: 10px;
  border-radius: 10px;
  margin-left: 30px;
}
.search-term {
  display: inline-block;
  border-radius: 10px;
  padding: 5px;
  color: white;
  margin-right: 5px;
}
.search-count {
  font-size: 18px;
}

.close{
  color: rgba(0,0,0,.4);
  margin: 20px;
}
.close :hover{
  color: "red";
}
#search-icon{
  font-size: 20px;
  color: rgba(0,0,0,.3);
  position: fixed;
  margin-left:60%;
  margin-top: -70px;
}
#search{
  margin-left:55%;
  background: rgba(255,255,255,.4);
  width: 60%;
  margin-top: -180px;
  position:fixed;
}
#word-search{
  margin: 2px;
  width:50px;
  margin:15px;
  background: white;
  border: 1px solid #ddd;
  position: absolute;
  border-radius: 90px;
}
#word-search :hover{
  background: 'red'
}
#search-input{
  border: none;
  margin-left: 40px;
  color: rgba(0,0,0,.5);
  width: 55%;
  height: 60px;
  background: rgba(255,255,255,.4);
  font-family: "Lucida Grande";
}
#extra-menu{
  margin: 30%;
  display: inline;
}
#legend{
  margin-left: 30px
}
#devise{
  margin-left: 100px;
  margin-top: 100px;
}

#google-maps-view{
  margin-left: 40px;
  height: 500px;
  width: 1080px;
}

#map-canvas{
  height: 100%;
  margin: 0;
  padding: 0;
}

#relephant-placeholder {
  font-size: 36px;
  height: 300px;
  text-align: center;
}

#relephant-placeholder img {
  margin-top: 100px;
  opacity: .7;
}
